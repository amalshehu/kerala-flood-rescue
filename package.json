{
  "name": "kerala-flood-rescue",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p build:watch credentials",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "build:prod:deploy": "npm run build:prod && npm run deploy",
    "build:browser:prod": "ng build --prod",
    "build:browser:serverless": "ng build --prod --base-href /",
    "build:serverless": "npm run build:browser:serverless && npm run build:server:serverless",
    "build:prod": "npm run build:client-and-server-bundles && npm run webpack:server",
    "server": "node local.js",
    "build:serverless:deploy": "npm run build:serverless && npm run deploy",
    "deploy": "cp-cli dist/ functions/dist/ && cd functions && npm install && firebase deploy",
    "build:server:prod": "ng run kerala-flood-rescue:server && webpack --config webpack.server.config.js --progress --colors",
    "build:server:serverless": "ng run kerala-flood-rescue:server && webpack --config webpack.server.config.js --progress --colors",
    "postinstall": "node credentials.js && opencollective postinstall",
    "credentials": "node credentials.js",
    "build:watch": "ng serve",
    "build:dev": "run-p test:watch start",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:client-and-server-bundles": "ng build --prod && ng run kerala-flood-rescue:server",
    "test:watch": "ng test --watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.3",
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.0.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.0.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/platform-server": "^6.0.0",
    "@angular/router": "^6.0.3",
    "@angular/service-worker": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular-instantsearch": "^2.0.3",
    "angularfire2": "^5.0.0-rc.11",
    "firebase": "^5.4.0",
    "geofire": "^4.1.2",
    "geofirex": "0.0.6",
    "core-js": "^2.5.4",
    "cors": "~2.8.4",
    "cp-cli": "^1.1.0",
    "instantsearch.js": "^2.10.1",
    "rxjs": "^6.0.0",
    "typescript-collections": "^1.3.2",
    "webpack-cli": "2.1.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^6.0.3",
    "@angular-devkit/build-angular": "~0.6.8",
    "typescript": "~2.7.2",
    "@angular/cli": "~6.0.8",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "0.13.19",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.7.0",
    "ts-loader": "4.2.0",
    "webpack-cli": "2.1.2",
    "opencollective": "^1.0.3",
    "cp-cli": "^1.1.0",
    "cpy-cli": "^1.0.1",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "express": "^4.15.2",
    "http-server": "^0.10.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "node-wget": "^0.4.2",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.10",
    "serverless": "1.26.1",
